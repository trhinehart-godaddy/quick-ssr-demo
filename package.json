{
  "name": "quick-ssr-demo",
  "private": true,
  "version": "1.0.0",
  "author": "trhinehart",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:server": "babel src --out-dir dist/src --ignore src/client",
    "build:webpack": "webpack --mode production",
    "start": "node dist/src/server/index.js",
    "prewatch": "npm run clean",
    "watch": "run-p watch:*",
    "watch:server": "nodemon --watch src/server --watch src/common --exec babel-node src/server/index.js",
    "watch:webpack": "webpack watch --mode development"
  },
  "dependencies": {
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "license": "MIT"
}
