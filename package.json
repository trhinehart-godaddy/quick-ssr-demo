{
  "name": "quick-ssr-demo",
  "private": true,
  "version": "1.0.0",
  "author": "trhinehart",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:client": "webpack --config webpack.client.config.js",
    "build:server": "webpack --config webpack.server.config.js",
    "start": "node dist/webpack/server/server.js",
    "prewatch": "npm run clean",
    "watch": "run-p watch:*",
    "watch:server": "nodemon --ext js,scss --watch src/server --watch src/common --exec \"npm run build:server -- --mode development && npm run start\"",
    "watch:client": "webpack watch --mode development --config webpack.client.config.js"
  },
  "dependencies": {
    "express": "^4.19.2",
    "linaria": "^1.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^7.2.9",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.1",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "sass": "^1.77.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "license": "MIT"
}
